<div class="page-content">
    {% block Estudio%}
    
    <table id="Estudio" class="table">
        
         <thead>
            <th width="10%">Region</th>
            <th width="10%">Puerto</th>
            <th width="10%">Código Cadena</th>
            <th width="10%">Nombre Cadena</th>
            
            
            <th width="10%">Factor</th>
            <th width="10%">Macro Proceso</th>
            <th width="10%">Actividad</th>
            <th width="10%">Costo unitario (us $ dolar)</th>
            <th widht="10%">Costo invitario (us $ dólar)</th>

        </thead>
        
       
        <tbody>
            {% for estudio in estudio %}
            <tr>
                <td>{{estudio.regiones.numero}}</td>
                <td>{{estudio.puertos.nombre}}</td>
                <td>{{estudio.cadenas.codigo}}</td>
                
                <td>{{estudio.cadena}}</td>
                <td>{{estudio.factor}}</td>
                <td>{{estudio.MACROPROCESO}}</td>
                <td>{{estudio.ACTIVIDAD}}</td>
                <td>{{estudio.COSTOunitario}}</td>
                <td>{{estudio.cOSTODEINVENTARIO}}</td>
                

            </tr>
            {% endfor%}
        </tbody>
    </table>
    {% endblock%}
</div>