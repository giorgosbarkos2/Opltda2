<table class="table" id="modelamiento">
    <thead>
        <th>Nombre puerto</th>
        <th>Nombre cadena</th>
        <th>Codigo cadena</th>
        <th>Visualizar Pdf</th>
        <th>Bajar Bpm</th>
 
    </thead>
    <tbody>
        {% for modelamiento in modelamiento %}
        <tr>
            <td>{{modelamiento.puertos.nombre}}</td>
            <td>{{modelamiento.cadenas.nombre}}</td>
            <td>{{modelamiento.cadenas.codigo}}</td>
            <td><a href="../web/archivos/modelamiento/{{modelamiento.archivoDoc}}" target="_blank">{{modelamiento.archivoDoc}}</a></td>
            <td><a href="../web/archivos/modelamiento/{{modelamiento.archivoBpm}}" target="_blank" >{{modelamiento.archivoBpm}}</a></td>
        </tr>
        {% endfor %}
    </tbody>
</table>
{% endblock %}
        </div><!-- #page-content -->
