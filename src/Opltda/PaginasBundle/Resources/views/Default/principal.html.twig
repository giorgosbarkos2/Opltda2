     <script src="http://maps.google.com/maps/api/js?sensor=false" 
          type="text/javascript"></script>

  <div id="map" style="width: 100%; height: 600px;"></div>

  <script type="text/javascript">
    var locations = [
        ['Puerto Valparaiso  <a href="detallePuerto/41">Ver detalle</a>', -33.032341,-71.628073, 4],
        ['Puerto Coquimbo   <a href="detallePuerto/30">Ver detalle</a> ' , -29.949664,-71.336129,4],
        ['Puerto San Antonio   <a href="detallePuerto/46">Ver detalle</a>', -33.58445,-71.614233 , 4],
        ['Puerto Bio Bio  <a href="detallePuerto/46">Ver detalle</a>', -36.713157,-73.109404 , 4],
     
     
        
      
    
    ];

    var map = new google.maps.Map(document.getElementById('map'), {
      zoom: 5,
      center: new google.maps.LatLng(-33.578014,-70.676881),
      mapTypeId: google.maps.MapTypeId.ROADMAP
    });

    var infowindow = new google.maps.InfoWindow();

    var marker, i;

    for (i = 0; i < locations.length; i++) {  
      marker = new google.maps.Marker({
        position: new google.maps.LatLng(locations[i][1], locations[i][2]),
        map: map
      });

      google.maps.event.addListener(marker, 'click', (function(marker, i) {
        return function() {
          infowindow.setContent(locations[i][0]);
          infowindow.open(map, marker);
        }
      })(marker, i));
    }
  </script>
    
